{
  "name": "etmgen",
  "version": "1.1.0",
  "description": "代码生成器",
  "repository": "https://github.com/take-easy-soft/easy-generator",
  "main": "dist/main.js",
  "dependencies": {
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "fs": "0.0.1-security"
  },
  "bin": "./dist/main.js",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.30.0",
    "nodemon": "^2.0.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3",
    "typescript-json-schema": "^0.42.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node dist/test/test.js",
    "generate": "npm run compile && npm run start",
    "debug": "nodemon --inspect ./dist/main.js",
    "start": "node dist/main.js ",
    "schema": "typescript-json-schema --refs true --required \"./src/type/*.ts\" Controller>res/schema.json"
  },
