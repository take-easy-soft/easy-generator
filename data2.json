{
  "name": "valid",
  "desc": "C端移动端组织相关的接口",
  "platform": "P",
  "userType": "B",
  "package": "com.etm.example",
  "author": "乌龙茶",
  "path": "/valid",
  "apis": [
    {
      "name": "validPathVariable",
      "desc": "校验路径参数",
      "path": "/path_variable/{name}",
      "method": "GET",
      "request": {
        "pathVar": [
          {
            "name": "name",
            "type": "String",
            "desc": "名称",
            "valid": {
              "type": "pattern",
              "pattern": "\\d{5}",
              "message": "名称格式不正确"
            }
          }
        ]
      },
      "response": {
        "body": [
          {
            "name": "result",
            "type": "String",
            "desc": "结果"
          }
        ]
      }
    }
  ]

}