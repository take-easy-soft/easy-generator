package <%=package%>;

import org.springframework.web.bind.annotation.*;
<% for(i=0;i<functions.length;i++){%>
import <%=functions[i].outEntityImport%>
import <%=functions[i].inEntityImport%>
<%}%>
/**
 * <%=description%>
 *
 * @author <%=author%>
 * @date <%=date%>
 */


@RestController
@RequestMapping("<%=mapping%>")
@Validated
public class <%=name%> {
	
	<% for(i=0;i<functions.length;i++){%>
	<% if (functions[i].method == "PUT") { %>
	@PutMapping("<%=functions[i].url%>") 
	<% }else if (functions[i].method == "POST") { %>
	@PostMapping("<%=functions[i].url%>") 
	<% } else if(functions[i].method == "GET") { %>
	 @GetMapping("<%=functions[i].url%>") 
	<% }else if (functions[i].method == "DELETE") { %>
	 @DeleteMapping("<%=functions[i].url%>") 	<% } %>
	public <%=functions[i].outEntityName%> <%=functions[i].methodName%>(<%=functions[i].inEntityName%> dto){
		return null;
	}
	<%}%>
}